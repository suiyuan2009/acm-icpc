
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef unsigned long long ull;
const int maxn = 405;
const int maxm = 200005;
const double eps = 1e-6;
const int inf = 0x3fffffff;
const int mod=1000000007;

int n,m,w[maxn],t[maxn];
ull f[maxm];
int pnt[maxm][55];
map<ull,int>mp;

int main()
{
    //freopen("ACM/in.in","r",stdin);
    for(int i=0;i<51;i++)mp[1LL<<i]=i;
    int T;
    scanf("%d",&T);
    while(T--)
    {
        scanf("%d%d",&n,&m);
        for(int i=1; i<=n; i++)scanf("%d%d",&w[i],&t[i]);
        memset(f,0,sizeof(f));
        memset(pnt,0,sizeof(pnt));
        f[0]=1;
        for(int i=1; i<=n; i++)
            for(int j=maxm-1; j>=0; j--)
            {
                if(f[j]==0)continue;
                if(j+t[i]>=maxm)continue;
                ull tt=(f[j]<<w[i])&((1LL<<51)-1);
                /*for(int k=0; k<51; k++)
                  if((tt&(1LL<<k))&&(!pnt[j+t[i]][k]))pnt[j+t[i]][k]=i;
                  f[j+t[i]]|=tt;*/
                ull tmp=f[j+t[i]];
                f[j+t[i]]|=tt;
                for(ull k=tmp^f[j+t[i]];k;k&=(k-1))
                {
                    ull x=k&(-k);
                    pnt[j+t[i]][mp[x]]=i;
                }
            }
        while(m--)
        {
            int x,y;
            scanf("%d%d",&x,&y);
            if(pnt[y][x])
            {
                bool sign=0;
                while(y>0)
                {
                    //printf("\n**%d %d\n",x,y);
                    if(sign)printf(" ");
                    else sign=1;
                    int tt=pnt[y][x];
                    printf("%d",tt);
                    //if(tt==-1)break;
                    y-=t[tt],x-=w[tt];
                }
                printf("\n");
            }
            else printf("No solution!\n");
        }
    }
    return 0;
}
